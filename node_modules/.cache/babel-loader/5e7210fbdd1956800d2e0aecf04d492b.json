AA,CAAA,SAAA,CAAA,MAAA,CAAA,SAAA;AACA,KA1BA;AA2BA,IAAA,WA3BA,uBA2BA,IA3BA,EA2BA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,aAAA,CAAA,OAAA,CAAA;AACA,MAAA,QAAA,CAAA,GAAA,GAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA,KA/BA;AAgCA,IAAA,QAhCA,sBAgCA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,KAlCA;AAmCA,IAAA,eAnCA,2BAmCA,IAnCA,EAmCA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,EAAA,CAAA;AACA;AArCA,GATA;AAgDA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,aAAA,KAAA,MAAA,CAAA,MAAA,KAAA,CAAA;AACA;AAHA;AAhDA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"row mb-5\">\n      <div class=\"col-12\">\n        <div\n          id=\"dragndrop-field\"\n          @drop.prevent=\"drop\"\n          @dragover.prevent=\"dragover\"\n          @dragleave=\"dragleave\"\n          class=\"z-depth-1-half p-3 cursor-pointer\"\n        >\n          <div>\n            <i class=\"fas fa-cloud-upload-alt\"></i>\n            <br />Drop your audio files here our files here\n            <br />\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"row mb-5\" v-if=\"!noFiles\">\n      <div class=\"col-12\">\n        <div\n          style=\"padding: 1.2rem;border: .1rem solid #bbcbd0;border-radius: .2rem;background-color: #fff;max-width: 96rem;\"\n        >\n          <div\n            v-for=\"(audio,index) of audios\"\n            :key=\"index\"\n            style=\"margin: .4rem;padding: 0 1.28rem;background: #f0f0f0;border: .1rem solid #cfdbde;border-radius: .2rem;font-weight: bold;\"\n          >\n            <span class=\"col-2\">{{index+1}}</span>\n            <span class=\"col-2\">{{audios[index].name}}</span>\n            <span class=\"col-2\"></span>\n\n            <span class=\"col-1\">\n              <i class=\"fas fa-trash-alt cursor-pointer\" @click=\"removeAudioFile(audio)\"></i>\n            </span>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-12\" id=\"button-area\">\n        <button\n          id=\"transcriptButton\"\n          class=\"float-left col-6 btn-block btn btn-primary\"\n          :disabled=\"noFiles\"\n        >\n          Transcript\n          <i class=\"fas fa-upload\"></i>\n        </button>\n        <button class=\"col-6 btn-block btn btn-danger\" @click=\"clearAll\">\n          Clear\n          <i class=\"fas fa-broom\"></i>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n<script>\n\nexport default {\n  name: \"dragNdrop\",\n\n  data() {\n    return {\n      audios: [],\n         modal: false\n    };\n  },\n  methods: {\n    dragover(event) {\n      event.currentTarget.classList.add(\"drag-bg\");\n    },\n    dragleave() {\n      event.currentTarget.classList.remove(\"drag-bg\");\n    },\n    drop(event) {\n      let droppedFiles = event.dataTransfer.files;\n\n      for (let file of droppedFiles) {\n        if (file.type.includes(\"audio\")) {\n          this.createAudio(file);\n        } else {\n         \n          this.$swal({\n            title:\"Was nein...?\",\n            text:\"You can't transcribe any other files than audio. Supported files are mp3, ogg, and acc.\",\n             icon: \"error\",\n              button: \"Ok. I understand... \"\n              //timer: 2000,\n          });\n        }\n      }\n\n      event.currentTarget.classList.remove(\"drag-bg\");\n    },\n    createAudio(file) {\n      let newAudio = document.createElement(\"audio\");\n      newAudio.src = URL.createObjectURL(file);\n      this.audios.push(file);\n    },\n    clearAll() {\n      this.audios = [];\n    },\n    removeAudioFile(file) {\n      this.audios.splice(this.audios.indexOf(file), 1);\n    },\n  },\n  computed: {\n    noFiles() {\n      return this.audios.length === 0;\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n#dragndrop-field:hover {\n  background-color: #eee;\n  transition: 0.3s ease-out;\n}\n#dragndrop-field {\n  height: 200px;\n}\n\n.drag-bg {\n  background-color: #e8eaf6;\n}\n\n.cursor-pointer {\n  cursor: pointer;\n}\n\n\n\n</style>\n\n"],"sourceRoot":"src/components"}]}